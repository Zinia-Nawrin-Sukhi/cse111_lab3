import java.util.Scanner;

  public class T2{
    public static void main(String[]args)
    {
      Scanner sc=new Scanner(System.in);
      System.out.println("please enter anumber");
      String s=sc.nextLine();
      for(int i=0;i<=s.length;i++){
     Char a=s.CharAt(i);
      System.out.println(a);
    }
    
}